    "eslint": "^7.18.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
